{
	"name": "extension-manager-gitlab",
	"displayName": "Extension Manager Gitlab",
	"description": "Manage your extensions using Gitlab packages",
	"version": "1.0.0",
	"publisher": "Delgadillox",
	"engines": {
		"vscode": "^1.92.0"
	},
	"icon": "logo.png",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Delgadillox/extension-manager"
	},
	"bugs": {
		"url": "https://github.com/Delgadillox/extension-manager/issues"
	},
	"homepage": "https://github.com/Delgadillox/extension-manager#readme",
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "extension-explorer",
					"title": "Extension Manager Explorer",
					"icon": "resources/icon.png"
				}
			]
		},
		"views": {
			"extension-explorer": [
				{
					"id": "extension-installed",
					"title": "Installed",
					"name": "Installed"
				},
				{
					"id": "extension-marketplace",
					"title": "Marketplace",
					"name": "Marketplace"
				}
			]
		},
		"configuration": [
			{
				"type": "object",
				"title": "Extension Settings",
				"properties": {
					"extension-manager.token": {
						"type": "string",
						"default": "",
						"description": "Your access token for authentication."
					},
					"extension-manager.projectId": {
						"type": "string",
						"default": "",
						"description": "Gitlab project ID"
					},
					"extension-manager.gitlabHost": {
						"type": "string",
						"default": "gitlab.com",
						"description": "Gitlab host"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/vscode": "^1.92.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"axios": "^1.7.4"
	}
}
